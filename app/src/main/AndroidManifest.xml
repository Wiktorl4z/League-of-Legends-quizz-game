<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.l4z.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/league_icon"
        android:label="LoLQuizz"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.MainMenu" />
        <activity android:name=".activitys.MainActivity" />
        <activity android:name=".activitys.FinalScreenActivity" />
    </application>
</manifest>